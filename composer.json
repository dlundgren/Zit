{
  "name": "inxilpro/zit",
  "description": "Zit is a simple dependency injector based heavily on Pimple. It aims to provide the same simplicity as Pimple while offering a slightly more robust object interface.",
  "version": "4.0.0",
  "type": "library",
  "keywords": [
    "dependency injection",
    "dependency",
    "di",
    "container",
    "zit",
    "pimple"
  ],
  "homepage": "https://github.com/inxilpro/Zit",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://cmorrell.com/"
    },
    {
      "name": "David Lundgren",
      "homepage": "https://www.davidscode.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/inxilpro/Zit/issues"
  },
  "autoload": {
    "psr-4": {
      "Zit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zit\\": "tests"
    }
  },
  "require": {
    "php": ">=7.2",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.32",
    "phpunit/phpunit": "^8.5 || ^9.3",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "sniff": "phpcs --standard=PSR12 src",
    "phpstan": "phpstan analyse src --memory-limit=1"
  }
}
